[Panel]
PanelA = title=已完成订阅制解锁,content=感谢您回到𝑺𝒖𝒓𝒈𝒆\n尊贵的𝑺𝒖𝒓𝒈𝒆 𝑷𝒓𝒐用户,icon =crown.fill,icon-color=#FFD700
 网络信息 = title=" 网络信息",content="请刷新",style=info,script-name= 网络信息
 𝒀𝒐𝒖𝑻𝒖𝒃𝒆 𝑷𝒓𝒆𝒎𝒊𝒖𝒎 解锁检测 = script-name= 𝒀𝒐𝒖𝑻𝒖𝒃𝒆 𝑷𝒓𝒆𝒎𝒊𝒖𝒎 解锁检测, title=" 𝒀𝒐𝒖𝑻𝒖𝒃𝒆 𝑷𝒓𝒆𝒎𝒊𝒖𝒎 解锁检测", update-interval=3600
 𝑵𝒆𝒕𝒇𝒍𝒊𝒙 𝑷𝒓𝒆𝒎𝒊𝒖𝒎 解锁检测 = script-name= 𝑵𝒆𝒕𝒇𝒍𝒊𝒙 𝑷𝒓𝒆𝒎𝒊𝒖𝒎 解锁检测, title=" 𝑵𝒆𝒕𝒇𝒍𝒊𝒙 𝑷𝒓𝒆𝒎𝒊𝒖𝒎 解锁检测", update-interval=3600
 𝑵𝒆𝒕𝒇𝒍𝒊𝒙 𝑷𝒓𝒆𝒎𝒊𝒖𝒎 自动切换 = script-name= 𝑵𝒆𝒕𝒇𝒍𝒊𝒙 𝑷𝒓𝒆𝒎𝒊𝒖𝒎 自动切换, update-interval=43200
♻️ Refresh DNS = script-name=♻️ Refresh DNS,title="♻️ 𝑹𝒆𝒇𝒓𝒆𝒔𝒉 𝑫𝑵𝑺", update-interval=3600
⚙️ 策略组控制 = script-name=⚙️ 策略组控制,update-interval=-1

[script]
# Surge 网络信息面板
 网络信息 = script-path=https://raw.githubusercontent.com/love499520/mask_angel/master/Net.js,type=generic
# YouTube 解锁检测
 𝒀𝒐𝒖𝑻𝒖𝒃𝒆 𝑷𝒓𝒆𝒎𝒊𝒖𝒎 解锁检测 = type=generic,script-path=https://raw.githubusercontent.com/love499520/mask_angel/master/YouTube.js
# Netflix 解锁检测
 𝑵𝒆𝒕𝒇𝒍𝒊𝒙 𝑷𝒓𝒆𝒎𝒊𝒖𝒎 解锁检测 = type=generic,script-path=https://raw.githubusercontent.com/love499520/mask_angel/master/Netflix.js
# Netflix 自动切换
 𝑵𝒆𝒕𝒇𝒍𝒊𝒙 𝑷𝒓𝒆𝒎𝒊𝒖𝒎 自动切换 = type=generic,script-path=https://raw.githubusercontent.com/love499520/mask_angel/master/nf_zd.js, argument=icon1=checkmark.circle&color1=#55ba94&icon2=checkmark.circle.trianglebadge.exclamationmark&color2=#9a9ced&icon3=xmark.shield&color3=#AF52DE&netflixGroup= 奈飞视频
# Refresh DNS
♻️ Refresh DNS = type=generic,script-path=https://raw.githubusercontent.com/love499520/mask_angel/master/RefreshDNS.js,argument=icon=arrow.triangle.2.circlepath.circle&color=#32CD32
# 信息面板控制
⚙️ 策略组控制 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/fishingworld/something/main/groupPanel.js,argument=icon=network&color=#86abee&group= 手动切换
  #对应参数：
  #icon：图标
  #color：图标颜色
  #group：策略组名称
  
